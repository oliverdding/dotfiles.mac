{
    "languageserver": {
        "go": {
            "command": "gopls",
            "args": [
                "serve",
                "-debug",
                "0.0.0.0:8484",
                "-rpc.trace"
            ],
            "rootPatterns": [
                "go.mod"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "go"
            ]
        },
        "rust": {
            "command": "rust-analyzer",
            "filetypes": [
                "rust"
            ],
            "rootPatterns": [
                "Cargo.toml"
            ]
        }
    },
    "explorer": {
        "icon": {
            "enableNerdfont": "true",
            "expanded": "",
            "collapsed": "",
            "selected": "",
            "hidden": "﬒"
        },
        "position": "right"
    }
}